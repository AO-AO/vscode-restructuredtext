{
	"code": {
		"prefix": "code",
		"body": ".. code-block:: ${1:language}\n$0",
		"description": "Code",
		"scope": "text.restructuredtext"
	},
	"image": {
		"prefix": "image",
		"body": ".. image:: ${1:path}\n$0",
		"description": "Image",
		"scope": "text.restructuredtext"
	},
	"link": {
		"prefix": "link",
		"body": "\n\\`${1:Title}<${2:http://link}>\\`_",
		"description": "Link",
		"scope": "text.restructuredtext"
	},
	"s1": {
		"prefix": "s1",
		"body": "${1:subsection name}\n${1/(.)|\\s/(?1:=:=)/g}\n$0",
		"description": "Section 1",
		"scope": "text.restructuredtext"
	},
	"s2": {
		"prefix": "s2",
		"body": "${1:subsection name}\n${1/(.)|\\s/(?1:*:*)/g}\n$0",
