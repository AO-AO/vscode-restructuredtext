		"description": "Section 6",
		"scope": "text.restructuredtext"
	},
	"s7": {
		"prefix": "s7",
		"body": "${1:subsection name}\n${1/(.)|\\s/(?1:~:~)/g}\n$0",
		"description": "Section 7",
		"scope": "text.restructuredtext"
